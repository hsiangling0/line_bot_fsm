digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	input_server [label="input_server\l"]
	input_place [label="input_place\l"]
	input_tag [label="input_tag\l"]
	show_recommand [label="show_recommand\l"]
	input_sour [label="input_sour\l"]
	input_non_sour_flavor [label="input_non_sour_flavor\l"]
	input_sour_flavor [label="input_sour_flavor\l"]
	show_non_sour_rm [label="show_non_sour_rm\l"]
	show_sour_rm [label="show_sour_rm\l"]
	user [label="user\l" color=red fillcolor=darksalmon peripheries=2]
	user -> input_server [label="advance [is_going_to_input_server]"]
	input_server -> input_place [label="advance [is_going_to_input_place]"]
	input_server -> input_sour [label="advance [is_going_to_input_sour]"]
	input_server -> user [label=go_back]
	input_place -> input_tag [label="advance [is_going_to_input_tag]"]
	input_place -> user [label=go_back]
	input_tag -> show_recommand [label="advance [is_going_to_show_recommand]"]
	input_tag -> user [label=go_back]
	show_recommand -> input_server [label="advance [is_going_to_input_server]"]
	show_recommand -> user [label=go_back]
	input_sour -> input_non_sour_flavor [label="advance [is_going_to_input_non_sour_flavor]"]
	input_sour -> input_sour_flavor [label="advance [is_going_to_input_sour_flavor]"]
	input_sour -> user [label=go_back]
	input_non_sour_flavor -> show_non_sour_rm [label="advance [is_going_to_show_non_sour_rm]"]
	input_non_sour_flavor -> user [label=go_back]
	input_sour_flavor -> show_sour_rm [label="advance [is_going_to_show_sour_rm]"]
	input_sour_flavor -> user [label=go_back]
	show_non_sour_rm -> input_server [label="advance [is_going_to_input_server]"]
	show_non_sour_rm -> user [label=go_back]
	show_sour_rm -> input_server [label="advance [is_going_to_input_server]"]
	show_sour_rm -> user [label=go_back]
}
